<%- include('layouts/header-k') %> 

<h1>Halaman Kelola Akun</h1>
<table class="table" border="">
    <thead>
      <tr>
          <th scope="col">NIK</th>
          <th scope="col">Nama</th>
          <th scope="col">Email</th>
          <th scope="col">Password</th>
          
      </tr>
    </thead>
    <tbody>
    <% for(let data of dosenData) { %>
    <tr>
        <td><%= data.NIK%></td>
        <td><%= data.name%></td>    
        <td><%= data.Email%></td>
        <td><%= data.Password%></td>  
        
        <td > 
          <div class="action">
            <a href="/edit/<%= data.id%>"><i class="fa-solid fa-pen-to-square fa-2x"></i></a>
            <a href="/delete/<%= data.id %>"onClick="return confirm('Apakah yakin akan menghapus?')"><i class="fa-solid fa-trash-can fa-2x"></i></a>
            
          </div>
        </td>         
    </tr>
    <% } %>
    </tbody>
  </table>
</body>
</html>